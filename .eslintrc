// Eslint Doc: http://eslint.org/docs/rules/
// 0: Close - 1: Warning - 2: Alert
{
    "rules": {
        "strict": 2,
        "no-unused-vars": 2,
        "no-undef": 2,
        "quotes": [2, "single"],
        "curly": 2,
        "eqeqeq": 0,
        "camelcase": 2,
        "no-console": 0,
        "no-eval": 2,
        "no-implied-eval": 2,
        "no-underscore-dangle": 0,
        "no-trailing-spaces": 2,
        "no-array-constructor": 2,
        "dot-notation": 2,
        "comma-style": [2, "last"],
        "brace-style": [2, "1tbs", {
            "allowSingleLine": true
        }],
        "space-before-blocks": [2, "always"],
        "space-in-parens": [2, "never"],
        "keyword-spacing": [2, {"before": true, "after": true, "overrides": {}}],
        "key-spacing": [2, {
            "beforeColon": false,
            "afterColon": true
        }]
    },
    "globals": {
        "console": true,
        "angular": true,
        "window": true,
        "document": true
    }
}
